{
  "id": "cancelReservation",
  "endPoints": [
  ],
  "outboundData": {
    "outboundEndPoints": [
      {
        "name": "launchCancelReservationWorkflow",
        "baseUrlType": "workflow",
        "url": "workflows/CancelReservationWorkflow/launch",
        "httpMethod": "POST",
        "authType": "sso",
        "onSend": "<%
          self.data;
        %>"
      }
    ]
  },
  "presentation": {
    "headerSize": "VPS_DEFAULT",
    "title": {
      "type": "title",
      "label": "Cancel Your Reservation"
    },
    "body": {
      "type": "section",
      "horizontal": false,
      "children": [
        {
          "type": "richText",
          "id": "confirmationMessage",
          "label": "Are you sure you want to cancel the reservation for <% queryParams.location %> on <% queryParams.date %>?",
          "value": "Click OK to cancel the reservation. Click cancel to abort."
        },
        {
          "type": "hidden",
          "id": "reservationId",
          "value": "<% queryParams.id %>",
          "valueOutBinding": "launchReserveSpotWorkflow.reservationId"
        },
        {
          "type": "hidden",
          "id": "locationId",
          "value": "<% queryParams.locationId %>",
          "valueOutBinding": "launchReserveSpotWorkflow.location"
        },
        {
          "type": "hidden",
          "id": "reservedById",
          "value": "<% queryParams.reservedBy %>",
          "valueOutBinding": "launchReserveSpotWorkflow.reservedBy"
        },
        {
          "type": "hidden",
          "id": "reservationDate",
          "value": "<% queryParams.reservationDate %>",
          "valueOutBinding": "launchReserveSpotWorkflow.reservationDate"
        }
      ]
    }
  }
}